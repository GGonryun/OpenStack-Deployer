#[Member]
ETCD_DATA_DIR="/var/lib/etcd/default.etcd"
ETCD_LISTEN_PEER_URLS="http://{{ hostvars[inventory_hostname]['ansible_host'] }}:2380"
ETCD_LISTEN_CLIENT_URLS="http://{{ hostvars[inventory_hostname]['ansible_host'] }}:2379"
ETCD_NAME="{{ hostvars[inventory_hostname]['ansible_facts']['hostname'] }}"

#[Clustering]
ETCD_INITIAL_ADVERTISE_PEER_URLS="http://{{ hostvars[inventory_hostname]['ansible_host'] }}:2380"
ETCD_ADVERTISE_CLIENT_URLS="http://{{ hostvars[inventory_hostname]['ansible_host'] }}:2379"
ETCD_INITIAL_CLUSTER="{{ hostvars[inventory_hostname]['ansible_facts']['hostname'] }}=http://{{ hostvars[inventory_hostname]['ansible_host'] }}:2380"
ETCD_INITIAL_CLUSTER_TOKEN="etcd-cluster-01"
ETCD_INITIAL_CLUSTER_STATE="new"